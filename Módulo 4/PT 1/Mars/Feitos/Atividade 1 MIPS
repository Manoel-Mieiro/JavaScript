.data
	Nome: .asciiz"Insira seu nome:		"	#nome
	DisplayNome: .space 30			# espaço de caracteres
	Resposta1: .asciiz "Seu nome e "	#saida do nome
	Endereco: .asciiz "Insira seu endereco:		"  #endereço
	DisplayEndereco: .space 50		# espaço de caracteres
	Resposta2: .asciiz "Seu endereco e "	#saida do endereço
	DataNascimento: .asciiz "Isira sua data de nascimento:		"	#data nasc	
	DisplayNascimento: .space 11		# espaço de caracteres
	Resposta3: .asciiz "Voce nasceu em "	#saida data nascimento


.text

li $v0, 4	#impressão da string
la $a0, Nome 	#endereço da mensagem
syscall		#impressão da mensagem
li $v0, 8	#leitura da string
la $a0, DisplayNome	#endereço da mensagem 
li $a1, 30	# define o espaço permitido
move $t0, $a0	#transere os dados o $a0 para o $t0
syscall		#impressão da mensagem

li $v0, 4	#impressão da string
la $a0, Endereco	#endereço da mensagem 
syscall			#impressão da mensagem
li $v0, 8		#leitura da string
la $a0, DisplayEndereco		#endereço da mensagem 
li $a1, 50	# define o espaço permitido
syscall		#impressão da mensagem

move $t1, $a0	#transere os dados o $a0 para o $t1
li $v0, 4	#impressão da string
la $a0, DataNascimento	#endereço da mensagem 
syscall		#impressão da mensagem

li $v0, 8	#leitura da string
la $a0, DisplayNascimento	#endereço da mensagem 
la $a1, 11	# define o espaço permitido
syscall		#impressão da mensagem

move $t2, $a0	#transere os dados o $a0 para o $t2
li $v0, 4	#impressão da string
la $a0, Resposta1	#endereço da mensagem 
syscall		#impressão da mensagem

li $v0, 4	#impressão da string
move $a0, $t0 	#transere os dados o $t0 para o $a0
syscall		#impressão da mensagem

li $v0, 4	#impressão da string
la $a0, Resposta2	#endereço da mensagem 
syscall		#impressão da mensagem

li $v0, 4	#impressão da string
move $a0, $t1 	#transere os dados o $t1 para o $a0
syscall		#impressão da mensagem

li $v0, 4	#impressão da string
la $a0, Resposta3	#endereço da mensagem 
syscall		#impressão da mensagem

li $v0, 4	#impressão da string
move $a0, $t2	#transere os dados o $t2 para o $a0
syscall		#impressão da mensagem






