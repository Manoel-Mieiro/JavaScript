# a = b - (c - d) + e
li $s1,  1 #carregar o valor IMEDIATO 1 no registrador $s1 (li = load immediate), o qual corresponde a "a"
 li $s2, 2	#carregar o valor IMEDIATO 2 no registrador $s1 (li = load immediate), o qual corresponde a "b"
 li $s3, 3	#carregar o valor IMEDIATO 3 no registrador $s1 (li = load immediate), o qual corresponde a "c"
 li $s4, 4	#carregar o valor IMEDIATO 4 no registrador $s1 (li = load immediate), o qual corresponde a "d"
 li $s5, 5	#carregar o valor IMEDIATO 5 no registrador $s1 (li = load immediate), o qual corresponde a "e"
 
  sub $s3, $s3, $s4 #
  sub $t0, $s2, $s3
  add $t1, $t0, $s5
  add $s1, $t1, $zero