#	int num, start=0
#	printf ("Insira até onde vai nossa sequencia");
#	scanf ("%d", &num);
#		while (start <= num){
#		printf ("%d", start);
#		printf ("\n");
#		start++
#		}

.data
	num: .asciiz	"Insira até onde vai nossa sequência:	"	#variável que recebe o número inteiro
	space: .asciiz 	" "							#impressão de espaços para dividir os termos visualmente
.text
move $t1, $zero			#carrega 0 para $t1
li $v0, 4			#imprimir string
la $a0, num			#carregar a string em $a0
syscall			#imprimir a sring de num
li $v0, 5		#le o inteiro do teclado	
syscall			#executa a leitura do núemro inserido

move $t0, $v0		#armazena o valor lido em $t0


Condition: 		#laço condicional
bgt $t1, $t0, Exit	#se $t1 for maior ou igual a $t0, o programa vai pra saída
li $v0, 1		#imprimir num inteiro
move $a0, $t1		#tranferir o valor de $t1 para $a0
syscall			#imprimir termo da sequeência

li $v0, 4		#imprimir string
la $a0, space		#tranferir para $a0 o espaço em branco
syscall			#impressão de space

#incremento
addi $t1, $t1, 1	#adiciona 1 ao valor de $t1 e o armazena em $t1
j Condition		#volta para o início do laço condicional

Exit:			#caso a condição não seja mais satisfeita, exit
li $v0, 10		#comando de saída
syscall			#saída
