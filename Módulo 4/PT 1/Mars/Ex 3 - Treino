.data
	Termos:.asciiz "Insira a quantidade de termos que gostaria de ter na sequencia."
	space: .asciiz " "
.text
li $v0, 4
la $a0, Termos
syscall
li $v0, 5
syscall
move $t0, $v0

li $s0, 0

Sequencia:
bgt $s0, $t0, Exit
li $v0, 1
move $a0, $s0
syscall
li $v0, 4
la $a0, space
syscall

addi $s0, $s0, 1
j Sequencia

Exit:
li $v0, 10
syscall
